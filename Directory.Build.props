<?xml version="1.0" encoding="utf-8"?>
<Project>

	<PropertyGroup>
		<SolutionDir Condition="'$(SolutionDir)' == ''">$(MSBuildThisFileDirectory)</SolutionDir>
		<SourceRoot Condition="'$(SourceRoot)' == ''">$(SolutionDir)modules\project64\Source\</SourceRoot>
		<BuildRoot Condition="'$(BuildRoot)' == ''">$(SolutionDir)Build</BuildRoot>
		<TargetRoot Condition="'$(TargetRoot)' == ''">$(SolutionDir)Target</TargetRoot>
	</PropertyGroup>

	<PropertyGroup Label="Configuration">
		<Platform Condition="'$(Platform)' == 'Win32'">x86</Platform>
		<Configuration Condition="'$(Configuration)' == ''">Debug</Configuration>
		<ProjectName Condition="'$(ProjectName)'==''">$(MSBuildProjectName)</ProjectName>

		<OutDir Condition="'$(ProjectName)' != ''">$(BuildRoot)\$(Platform)\$(Configuration)\$(ProjectName)\</OutDir>
		<IntDir Condition="'$(ProjectName)' != ''">$(TargetRoot)\$(Platform)\$(Configuration)\$(ProjectName)\</IntDir>
	</PropertyGroup>

</Project>
