# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = false
indent_size = 4
indent_style = tab

[*.{config,psproj,vcxproj}]
end_of_line = crlf
indent_size = 2
indent_style = space

[*.{props,targets}]
indent_size = 2

[*.sln]
end_of_line = crlf
